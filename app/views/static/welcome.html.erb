<div id="fb-root"></div>
<script>(function(d, s, id) {
  var js, fjs = d.getElementsByTagName(s)[0];
  if (d.getElementById(id)) return;
  js = d.createElement(s); js.id = id;
  js.src = "//connect.facebook.net/en_US/sdk.js#xfbml=1&version=v2.0";
  fjs.parentNode.insertBefore(js, fjs);
  }(document, 'script', 'facebook-jssdk'));</script>
<div  class="container-fluid main_page_search_container">
  <%= form_tag display_subjects_path, method: 'get', id: 'main_subject_search', class: 'form-inline' do %>
    <div class="form-group">
      <%= text_field_tag :search_subjects, params[:search], placeholder: 'Subject name', class: 'typeahead subject', autocomplete: :off %>
    </div>
    <div class="form-group">
      <%= text_field_tag :search_position, params[:search], placeholder: 'County', class: 'typeahead county', autocomplete: :off %>
    </div>
    <%= submit_tag 'Search', name: 'nil', id: 'search_submit', class: 'btn btn-default' %>
  <% end %>
  <div id="display_search_results">

  </div>
</div> <!-- end of container-fluid main_page_search -->

<div class="fb-share-button" data-href="<%= request.original_url %>"></div>